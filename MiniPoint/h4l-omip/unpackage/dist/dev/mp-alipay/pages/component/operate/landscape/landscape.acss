
page {
		font-weight: normal;
}
.fui-page__bd {
		display: flex;
		align-items: center;
		justify-content: center;
		flex-direction: column;
}
.fui-ani__box {
		width: 640rpx;
		background: #FFFFFF;
		border-radius: 24rpx;
		padding-bottom: 64rpx;
}
.fui-hd__img {
		width: 100%;
		height: 192rpx;
		display: block;
}
.fui-ani__title {
		font-size: 36rpx;
		font-weight: 600;
		padding: 54rpx 0 32rpx;
		text-align: center;
}
.fui-desc {
		padding: 0 54rpx 82rpx;
		font-size: 24rpx;
		line-height: 48rpx;
		font-weight: 400;
		color: #333333;
		box-sizing: border-box;
}
.fui-desc text {
		color: #FF2B2B;
}
