
page {
		font-weight: normal;
}
.fui-section__title {
		margin-left: 32rpx;
}
.fui-list__cell {
		width: 100%;
		display: flex;
		align-items: center;
		justify-content: space-between;
}
.fui-text {
		font-size: 30rpx;
		padding-left: 16rpx;
}
.fui-section__title:not(:first-child) {
		margin-top: 64rpx;
}
.fui-clause--cell {
		display: flex;
		align-items: center;
		justify-content: center;
		font-size: 24rpx;
}
.fui-clause--wrap {
		width: 100%;
		margin-top: 64rpx;
}
.fui-clause--text {
		padding-left: 16rpx;
}
.fui-color__link {
		color: var(--fui-color-link);
}
.fui-color__link:active {
		opacity: .5;
}
.fui-btn__box {
		width: 100%;
		padding: 64rpx 32rpx;
		box-sizing: border-box;
}
