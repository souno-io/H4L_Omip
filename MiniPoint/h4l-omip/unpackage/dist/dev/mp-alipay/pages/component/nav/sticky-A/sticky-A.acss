
page {
		font-weight: normal;
}
.fui-section__title {
		margin-left: 32rpx;
		margin-top: 48rpx;
}
.fui-header {
		width: 100%;
		padding: 24rpx;
		box-sizing: border-box;
		display: flex;
		justify-content: center;
		background: #F8F8F8;
		color: #465CFF;
		font-weight: bold;
}
.fui-seat__wrap,
	.fui-footer {
		height: 60px;
		font-size: 24rpx;
		color: #B2B2B2;
		padding: 32rpx;
		box-sizing: border-box;
}
.fui-footer {
		position: absolute;
		left: 0;
		bottom: 0;
}
.fui-scroll__view {
		width: 100%;
		height: 200px;
		background: #fff;
}
