
.fui-page__bd {

		display: flex;

		align-items: center;
		justify-content: center;
		flex-direction: column;
}
.fui-item {

		display: flex;

		align-items: center;
		justify-content: center;
		flex-direction: column;
}
.fui-img {
		width: 120rpx;
		height: 120rpx;
		margin-bottom: 12rpx;
		border-radius: 16rpx;
		pointer-events: none;
}
.fui-text {
		font-size: 24rpx;
		color: #999;
		font-weight: normal;
}
