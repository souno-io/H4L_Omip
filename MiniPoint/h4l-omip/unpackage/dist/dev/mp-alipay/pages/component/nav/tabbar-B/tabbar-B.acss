@import "../../../../nvue.acss";

page {
		font-weight: normal;
}
.fui-wrap {




		width: 100%;
		padding-bottom: constant(safe-area-inset-bottom);
		padding-bottom: env(safe-area-inset-bottom);
		box-sizing: border-box;
}
.fui-swiper__wrap {




		flex-direction: column;
		height: 100vh;
}
.fui-swiper__item {



		flex-direction: row;
}
.fui-scroll__wrap {

		width: 100%;
}
.fui-tabbar__h {
		width: 750rpx;
}
.fui-text {
		font-size: 28rpx;
		color: #B2B2B2;
		padding: 32rpx;

		box-sizing: border-box;
}
