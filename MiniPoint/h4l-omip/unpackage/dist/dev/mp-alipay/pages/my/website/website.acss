
.fui-banner__box{
		width: 100%;
		height: 420rpx;
		background: #EDF9FF;
}
.fui-banner {
		width: 100%;
		height: 420rpx;
		display: block;
}
.fui-list__view {
		width: 100%;
		padding: 8rpx 32rpx 32rpx;
		box-sizing: border-box;
}
.fui-list__item {
		width: 100%;
		height: 112rpx;
}
.fui-list__item text {
		padding-left: 24rpx;
		padding-right: 16rpx;
}
.fui-item__icon {
		width: 48rpx;
		height: 48rpx;
}
