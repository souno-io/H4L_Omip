
page {
		background-color: #fff;
		font-weight: normal;
}
.fui-logo__box {
		width: 100%;
		display: flex;
		align-items: center;
		flex-direction: column;
		justify-content: center;
		padding-top: 64rpx;
		padding-bottom: 64rpx;
}
.fui-logo {
		width: 120rpx;
		height: 120rpx;
		border-radius: 24rpx;
}
.fui-title {
		font-size: 36rpx;
		padding-top: 24rpx;
}
.fui-version {
		padding-top: 8rpx;
		font-size: 24rpx;
		font-weight: 400;
}
.fui-desc {
		font-size: 28rpx;
		line-height: 56rpx;
		padding: 0 32rpx;
		margin-bottom: 64rpx;
		box-sizing: border-box;
		font-weight: 400;
}
.fui-mr {
		margin-right: 8rpx;
}
