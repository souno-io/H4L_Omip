@import "../../../nvue.acss";

page {
		font-weight: normal;
}
.fui-align__center {
		flex-direction: row;
}
.fui-section__title {
}
.fui-btn__box{

		width: 100%;
		display: flex;

		align-items: center;
		justify-content: center;
		padding-top: 64rpx;
}

	/*
	  访问记录
	*/
.fui-card__wrap {

		width: 100%;

		border-radius: 16rpx;
		overflow: hidden;
		background-color: #fff;
}
.fui-card__header {
		flex: 1;

		display: flex;
		width: 100%;

		flex-direction: row;
		justify-content: space-between;
}
.fui-scroll__view {
		flex-direction: row;
		overflow: hidden;
}
.fui-cell__list {

		display: flex;

		flex-direction: row;
}
.fui-list__item {
		width: 160rpx;

		display: flex;
		box-sizing: border-box;

		flex-direction: column;
		align-items: center;
		padding: 0 16rpx 24rpx;
}
