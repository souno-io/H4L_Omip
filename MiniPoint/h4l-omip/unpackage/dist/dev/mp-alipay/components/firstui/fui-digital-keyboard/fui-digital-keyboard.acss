
.fui-digital__keyboard-wrap.data-v-0c79ec00 {

		width: 100%;
		visibility: hidden;
		transform: translate3d(0, 100%, 0);
		transition-property: transform, visibility;
		transition-duration: .25s;
		transition-timing-function: ease-in-out;



		padding-bottom: constant(safe-area-inset-bottom);
		padding-bottom: env(safe-area-inset-bottom);

		position: fixed;
		left: 0;
		right: 0;
		bottom: 0;
		transform-origin: center center;
}
.fui-dk__theme-light.data-v-0c79ec00 {
		background: #F8F8F8;
}
.fui-dk__theme-dark.data-v-0c79ec00 {
		background: #1B1B1B;
}
.fui-digital__keyboard-wrap.data-v-0c79ec00::before {
		content: " ";
		position: absolute;
		top: 0;
		right: 0;
		left: 0;
		transform: scaleY(0.5) translateZ(0);
		transform-origin: 0 0;
		z-index: 2;
		pointer-events: none;
}
.fui-dk__theme-light.data-v-0c79ec00::before {
		border-top: 1px solid rgba(51, 51, 51, .1);
}
.fui-dk__theme-dark.data-v-0c79ec00::before {
		border-top: 1px solid #333;
}
.fui-digital__keyboard__ani.data-v-0c79ec00 {

		transform: translate3d(0, 0, 0);
		visibility: visible;
}
