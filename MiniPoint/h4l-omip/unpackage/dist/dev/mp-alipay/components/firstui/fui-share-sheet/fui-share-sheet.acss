
.fui-share__sheet-mask.data-v-22d34b15 {
		position: fixed;
		top: 0;
		left: 0;
		right: 0;
		bottom: 0;

		transition: all 0.25s ease-in-out;
		visibility: hidden;






		opacity: 0;
}
.fui-ss__mask-show.data-v-22d34b15 {

		visibility: visible;

		opacity: 1 !important;
}
.fui-share__sheet-wrap.data-v-22d34b15 {

		width: 100%;
		visibility: hidden;
		min-height: 100rpx;

		position: fixed;
		left: 0;
		right: 0;
		bottom: 0;

		transform: translate3d(0, 100%, 0);
		transition: all 0.25s ease-in-out;







		transform-origin: center center;
		flex-direction: column;
}
.fui-share__sheet-radius.data-v-22d34b15 {
		border-top-left-radius: 24rpx;
		border-top-right-radius: 24rpx;

		overflow: hidden;
}
.fui-share__sheet-show.data-v-22d34b15 {

		transform: translate3d(0, 0, 0);
		visibility: visible;
}
.fui-share__sheet-title.data-v-22d34b15 {

		width: 100%;
		display: flex;

		flex: 1;
		align-items: center;
		justify-content: center;
		text-align: center;
		font-size: 24rpx;
		color: #7F7F7F;
		padding: 24rpx 0;
		font-weight: normal;
}
.fui-share__sheet-box.data-v-22d34b15 {






		padding: 24rpx 0;
		opacity: 1;
}
.fui-share__sheet-scroll.data-v-22d34b15 {



		flex-direction: row;
}
.fui-share__sheet-list.data-v-22d34b15 {

		white-space: nowrap;
		display: inline-flex;
		box-sizing: border-box;

		flex-direction: row;
		align-items: center;
		padding: 0 20rpx;
}
.fui-share__sheet-full.data-v-22d34b15 {

		min-width: 100%;
		box-sizing: border-box;

		justify-content: space-between;
}
.fui-share__sheet-item.data-v-22d34b15 {

		display: flex;
		flex-shrink: 0;

		flex-direction: column;
		align-items: center;
		justify-content: center;
		padding: 0 16rpx;
}
.fui-share__sheet-item.data-v-22d34b15:active {
		opacity: .5;
}
.fui-share__sheet-icon.data-v-22d34b15 {

		display: flex;
		transform: translateZ(0);

		border-radius: 24rpx;
		align-items: center;
		justify-content: center;
		overflow: hidden;
}
.fui-share__sheet-img.data-v-22d34b15 {

		display: block;

