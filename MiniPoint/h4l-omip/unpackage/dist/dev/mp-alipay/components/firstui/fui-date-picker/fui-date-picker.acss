
.fui-date__picker-mask.data-v-5a18b311 {
		position: fixed;
		top: 0;
		left: 0;
		right: 0;
		bottom: 0;

		transition: all 0.25s ease-in-out;
		visibility: hidden;






		opacity: 0;
}
.fui-dpk__mask-show.data-v-5a18b311 {

		visibility: visible;

		opacity: 1;
}
.fui-date__picker-content.data-v-5a18b311 {

		width: 100%;
		visibility: hidden;

		position: fixed;
		left: 0;
		right: 0;
		bottom: 0;

		transform: translate3d(0, 100%, 0);
		transition: all 0.25s ease-in-out;








		transform-origin: center center;
		flex-direction: column;
		background: #fff;
}
.fui-dpk__content-dark.data-v-5a18b311 {
		background: #222;
}
.fui-dpk__content-show.data-v-5a18b311 {

